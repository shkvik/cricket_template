<div class="nurseryStyle_modal close" id="incubationModal">
    <div class="nurseryStyle_modal_container">

        <div class="btn_close" id="btn_close_add_incubation_box">
            <svg width="15" height="15" viewBox="0 0 15 15" fill="none" xmlns="http://www.w3.org/2000/svg">
                <path d="M0.75383 13.9534L7.36583 7.36603M13.9778 0.778701L7.36583 7.36603M7.36583 7.36603L0.759561 0.772971M7.36583 7.36603L13.9721 13.9591" stroke="white" stroke-opacity="0.8" stroke-width="1.2" stroke-linecap="round" stroke-linejoin="round"/>
            </svg>
        </div>

        <div class="nurseryStyle_modal_container_title">
            Add Incubation box 
        </div>

        <form class="nurseryStyle_modal_form" method="post" action="/nursery/addIncubationBox">

            <label>Fly cage ID</label>
            <input name="fly_cage_id" required="required" type="number" placeholder="Number of bins"/>

            <label>Weight of eggs, g</label>
            <input name="weight_of_eggs" required="required" type="number" placeholder="Number of bins"/>
            
            <label>Incubation box</label>
            <select name="incubation_box" class="incubation_box">
                <option value="">--Please choose an option--</option>
                <option value="1">1</option>
                <option value="2">2</option>
                <option value="3">3</option>
                <option value="4">4</option>
                <option value="5">5</option>
                <option value="6">6</option>
            </select>

            <button class="btn_add_submit" type="submit"> Add </button>
        </form>
        
    </div>
</div>


<div class="nurseryStyle_modal close" id="neonatesModal">
    <div class="nurseryStyle_modal_container" style="height: 700px;">

        <div class="btn_close" id="btn_close_add_neonates_tray">
            <svg width="15" height="15" viewBox="0 0 15 15" fill="none" xmlns="http://www.w3.org/2000/svg">
                <path d="M0.75383 13.9534L7.36583 7.36603M13.9778 0.778701L7.36583 7.36603M7.36583 7.36603L0.759561 0.772971M7.36583 7.36603L13.9721 13.9591" stroke="white" stroke-opacity="0.8" stroke-width="1.2" stroke-linecap="round" stroke-linejoin="round"/>
            </svg>
        </div>

        <div class="nurseryStyle_modal_container_title">
            Add Neonates tray 
        </div>

        <form class="nurseryStyle_modal_form" method="post" action="/nursery/addNeonatesTrays">
            <label>Incubation box ID</label>
            <input name="ncubation_box_id" required="required" type="number" placeholder="Number of bins"/>

            <label>Colony ID</label>
            <input name="colony_id" required="required" type="number" placeholder="Number of bins"/>

            <label>Weight of neonates, g</label>
            <input name="weight_of_neonates" required="required" type="number" placeholder="Number of bins"/>
            
            <label>Neonates tray</label>
            <select name="incubation_box" class="incubation_box">
                <option value="">--Please choose an option--</option>
                <option value="1">1</option>
                <option value="2">2</option>
                <option value="3">3</option>
                <option value="4">4</option>
                <option value="5">5</option>
                <option value="6">6</option>
            </select>

            <button class="btn_add_submit" type="submit"> Add </button>
        </form>
        
    </div>
</div>


